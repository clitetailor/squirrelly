{
  "name": "squirrelly",
  "version": "7.5.0",
  "description": "Simple and powerful template engine that supports helpers, partials, filters, native code, and Express.",
  "main": "./lib/index.js",
  "homepage": "https://squirrelly.js.org",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "test": "standard --fix src/**/*.js && mocha -r @babel/register",
    "build": "gulp build",
    "format": "standard --fix src/**/*.js"
  },
  "keywords": [
    "template",
    "engine",
    "squirrelly",
    "html",
    "expressjs",
    "sqrl",
    "pug",
    "handlebars",
    "swig",
    "filters",
    "helpers"
  ],
  "author": "Ben Gubler <nebrelbug@gmail.com> (https://bengubler.com)",
  "license": "MIT",
  "bin": {
    "squirrelly": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/squirrellyjs/squirrelly.git"
  },
  "bugs": {
    "url": "https://github.com/squirrellyjs/squirrelly/issues"
  },
  "dependencies": {
    "core-js": "^3.2.1",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "mocha": "^5.1.1",
    "prettier": "^1.18.2",
    "rollup": "^1.16.7",
    "rollup-plugin-terser": "^5.1.1",
    "standard": "^12.0.1"
  },
  "standard": {
    "ignore": [
      "test/*.js",
      "dist/*.js"
    ]
  },
  "browser": {
    "fs": false
  }
}
